{
  "name": "xbull",
  "version": "1.11.0",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "build:extension": "./node_modules/.bin/ng build --configuration extension",
    "build:extension:prod": "./node_modules/.bin/ng build --configuration extension-production",
    "build:extension:watch": "./node_modules/.bin/ng build --configuration extension --watch",
    "build:extension:platform-files:v2": "cross-env MANIFEST_VERSION=v2 ./node_modules/.bin/webpack-cli -c webpack.extension.js",
    "build:extension:platform-files:v2:watch": "cross-env MANIFEST_VERSION=v2 ./node_modules/.bin/webpack-cli -c webpack.extension.js --watch",
    "build:extension:platform-files:v3": "cross-env MANIFEST_VERSION=v3 ./node_modules/.bin/webpack-cli -c webpack.extension.js",
    "build:extension:platform-files:v3:watch": "cross-env MANIFEST_VERSION=v3 ./node_modules/.bin/webpack-cli -c webpack.extension.js --watch",
    "build:watch": "./node_modules/.bin/ng build --watch",
    "build:prod": "./node_modules/.bin/ng build --configuration production",
    "build:prod:watch": "./node_modules/.bin/ng build --configuration production --watch",
    "build:mobile": "./node_modules/.bin/ng build --configuration mobile",
    "build:mobile:plugins": "./node_modules/.bin/jetifier",
    "build:mobile:watch": "./node_modules/.bin/ng build --configuration mobile --watch",
    "build:mobile:prod": "./node_modules/.bin/ng build --configuration mobile-production",
    "build:mobile:resources:ios": "./node_modules/.bin/cordova-res ios --skip-config --copy --icon-background-source='#14142B'",
    "build:mobile:resources:android": "./node_modules/.bin/cordova-res android --skip-config --copy --icon-background-source='#14142B'",
    "deploy:firefox": "cross-env NODE_ENV=production npm run build:extension:prod && npm run build:extension:platform-files:v2",
    "deploy:chrome": "cross-env NODE_ENV=production npm run build:extension:prod && npm run build:extension:platform-files:v3",
    "deploy:android": "cross-env NODE_ENV=production npm run build:mobile:prod && npm run build:mobile:resources:android && npm run build:mobile:plugins && npm run sync:android",
    "deploy:ios": "cross-env NODE_ENV=production npm run build:mobile:prod && npm run build:mobile:resources:ios && npm run build:mobile:plugins && npm run sync:ios",
    "deploy:website": "cross-env NODE_ENV=production npm run build:prod",
    "sync:android": "./node_modules/.bin/cap sync android",
    "sync:ios": "./node_modules/.bin/cap sync ios",
    "test": "./node_modules/.bin/ng test",
    "lint": "./node_modules/.bin/ng lint",
    "e2e": "./node_modules/.bin/ng e2e"
  },
  "private": true,
