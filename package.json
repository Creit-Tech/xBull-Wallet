    "@types/webextension-polyfill": "^0.8.3",
    "autoprefixer": "^10.4.7",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "9.1.0",
    "cordova-res": "^0.15.4",
    "cross-env": "^7.0.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jetifier": "^2.0.0",
    "karma": "^6.3.20",
