    "@trezor/rollout": "^1.2.1",
    "bignumber.js": "^9.0.2",
    "bip39": "^3.0.3",
    "cordova-plugin-android-fingerprint-auth": "^1.5.0",
    "cordova-plugin-ios-keychain": "^3.0.1",
    "cordova-plugin-qrscanner": "^3.0.1",
    "cordova-plugin-touch-id": "^3.4.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "next-tick": "^1.1.0",
    "ng-zorro-antd": "^12.1.1",
    "ngx-mask": "^11.1.5",
    "qrcode": "^1.5.0",
    "rxjs": "~6.6.0",
    "stellar-sdk": "^9.1.0",
    "trezor-connect": "^8.2.6-extended",
    "tslib": "^2.0.0",
