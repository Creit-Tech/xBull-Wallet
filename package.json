{
  "name": "xbull",
  "version": "0.8.9",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "build:extension:v2": "cross-env MANIFEST_VERSION=v2 ./node_modules/.bin/webpack-cli -c webpack.extension.js",
    "build:extension:v2:watch": "cross-env MANIFEST_VERSION=v2 ./node_modules/.bin/webpack-cli -c webpack.extension.js --watch",
    "build:extension:v3": "cross-env MANIFEST_VERSION=v3 ./node_modules/.bin/webpack-cli -c webpack.extension.js",
    "build:extension:v3:watch": "cross-env MANIFEST_VERSION=v3 ./node_modules/.bin/webpack-cli -c webpack.extension.js --watch",
    "build:watch": "./node_modules/.bin/ng build --watch",
    "build:prod": "./node_modules/.bin/ng build --configuration production",
    "build:mobile": "./node_modules/.bin/ng build --configuration mobile",
    "build:mobile:plugins": "./node_modules/.bin/jetifier",
    "build:mobile:watch": "./node_modules/.bin/ng build --configuration mobile --watch",
    "build:mobile:prod": "./node_modules/.bin/ng build --configuration mobile-production",
    "build:mobile:resources:ios": "./node_modules/.bin/cordova-res ios --skip-config --copy --icon-background-source='#14142B'",
    "build:mobile:resources:android": "./node_modules/.bin/cordova-res android --skip-config --copy --icon-background-source='#14142B'",
    "deploy:firefox": "cross-env NODE_ENV=production npm run build:prod && npm run build:extension:v2",
    "deploy:chrome": "cross-env NODE_ENV=production npm run build:prod && npm run build:extension:v3",
    "deploy:android": "cross-env NODE_ENV=production npm run build:mobile:prod && npm run build:mobile:resources:android && npm run build:mobile:plugins",
    "deploy:ios": "cross-env NODE_ENV=production npm run build:mobile:prod && npm run build:mobile:resources:ios && npm run build:mobile:plugins",
    "test": "./node_modules/.bin/ng test",
    "lint": "./node_modules/.bin/ng lint",
    "e2e": "./node_modules/.bin/ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@awesome-cordova-plugins/core": "^5.39.0",
    "@awesome-cordova-plugins/keychain": "^5.39.0",
    "@awesome-cordova-plugins/touch-id": "^5.39.0",
    "@capacitor/android": "3.2.5",
    "@capacitor/app": "1.0.3",
    "@capacitor/core": "3.2.4",
    "@capacitor/haptics": "1.1.0",
    "@capacitor/ios": "3.3.3",
    "@capacitor/keyboard": "1.1.0",
    "@capacitor/status-bar": "1.0.3",
    "@capacitor/storage": "^1.2.4",
    "@datorama/akita": "^6.2.4",
    "@datorama/akita-ng-router-store": "^6.0.0",
    "@ionic-native/android-fingerprint-auth": "^5.36.0",
    "@ionic-native/core": "^5.36.0",
