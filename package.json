    "@types/chrome": "0.0.134",
    "@types/crypto-js": "^4.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^12.20.43",
    "@types/qrcode": "^1.4.2",
    "@types/w3c-web-usb": "^1.0.5",
    "@types/webextension-polyfill": "^0.8.2",
    "codelyzer": "^6.0.0",
    "copy-webpack-plugin": "9.1.0",
    "cordova-res": "^0.15.4",
    "cross-env": "^7.0.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jetifier": "^2.0.0",
    "karma": "^6.3.15",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^8.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webextension-polyfill": "^0.8.0",
    "webpack-cli": "^4.9.2"
  },
  "akitaCli": {
    "idKey": "_id",
    "customFolderName": "true",
    "template": "angular"
  }
}
