    "bip39": "^3.0.3",
    "cordova-plugin-android-fingerprint-auth": "^1.5.0",
    "cordova-plugin-ios-keychain": "^3.0.1",
    "cordova-plugin-qrscanner": "^3.0.1",
    "cordova-plugin-touch-id": "^3.4.0",
